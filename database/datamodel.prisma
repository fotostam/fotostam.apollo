type Order {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  name: String
  group: String
  status: Status
  error: String
  photos: [Photo!]!
}

type Photo {
  id: ID! @id
  tag: String!
  amount: Int!
  status: PhotoStatus!
  error: String
  url: String
  order: Order! @relation(link: INLINE)
}

enum PhotoStatus {
  HEALTHY
  ERROR
}

enum Status {
  ON_HOLD
  IN_PRODUCTION
  TO_BE_PICKED
  ERROR
  DONE
}